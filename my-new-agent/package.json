{
  "name": "forta-joemasterchefv3-agent",
  "version": "0.0.1",
  "description": "Forta Agent Typescript starter project",
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,ts,json  --exec \"npm run build && forta-agent run\"",
    "start:prod": "forta-agent run --prod",
    "tx": "npm run build && forta-agent run --tx",
    "block": "npm run build && forta-agent run --block",
    "range": "npm run build && forta-agent run --range",
    "file": "npm run build && forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "bignumber.js": "^9.0.1",
    "ethers": "^5.5.4",
    "forta-agent": "^0.0.39"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/nodemon": "^1.19.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.8",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  },
  "main": "jest.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dezzydez007/Forta-Agent-Smith.git"
  },
  "keywords": [
    "forta-agent-joe"
  ],
  "author": "dezzydez#7573",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dezzydez007/Forta-Agent-Smith/issues"
  },
  "homepage": "https://github.com/dezzydez007/Forta-Agent-Smith#readme"
}
